{
  "manifest_version": 3,
  "name": "URL Access Detector",
  "version": "1.0",
  "description": "Detects URL access and sends a message if the URL is not in the stored list",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
